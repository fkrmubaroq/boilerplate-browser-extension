{
  "manifest_version": 3,
  "name": "React Extension",
  "version": "1.0.0",
  "description": "React Extension",
  "action":{
    "default_popup": "popup.html"
  },
  "background":{
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}